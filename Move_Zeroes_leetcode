class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        
        int la = 0 , n=nums.size();
        for(int i=0; i<n; i++)
        {
            if(nums[i] != 0)
            {
                swap(nums[la] , nums[i]);
                la++;
                
            }
        }
    }
};
